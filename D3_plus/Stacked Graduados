<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width">
  <title>JS Bin</title>
</head>
<body>
<script src="//d3plus.org/js/d3.js"></script>
<script src="//d3plus.org/js/d3plus.js"></script>

<div id="viz"></div>

<script>
  
var url="https://raw.githubusercontent.com/aaizemberg/vis/gh-pages/graduados/data.tsv";  

  d3plus.viz()
    .container("#viz")
    .data(url)
    .type("stacked")
    .id("titulo")
    .size("cant")
    .time("año")
     .x("año")
     .y("cant")
     .shape({"interpolate":"monotone"})
    .format("es_ES")
    .draw()
</script>
</body>
</html>
